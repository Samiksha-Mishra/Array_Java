class Solution {
    public void moveZeroes(int[] nums) {
        int uniqueIndex = 0 , n = nums.length;
        for(int i = 0 ; i< n ; i++){
            if(nums[i]!=0){
                nums[uniqueIndex] = nums[i];
                uniqueIndex++;
            }
        }
        for(int i =uniqueIndex ; i< n ; i++){
            nums[i]=0;
        }
    }
}
