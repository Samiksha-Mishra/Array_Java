class Solution {
    public int removeDuplicates(int[] nums) {
        int i = 0 , j = 1 , n = nums.length,k=1;
        while(i<n&&j<n){
            if(nums[i]==nums[j]) j++;
    
            else {
                i++;
                int temp = nums[i];
                nums[i] = nums[j];
                nums[j] = temp;
                k++;
                j++;
            }
        }
        return k;
    }
}
