class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int n = nums.length, count=0, max1=0;
        for(int i =0 ; i< n ; i++){
            if(nums[i]==1){
                count++;
            }
            else{
                count=0;
            }
            max1 = Math.max(count, max1);
        }
        return max1;
    }
}
